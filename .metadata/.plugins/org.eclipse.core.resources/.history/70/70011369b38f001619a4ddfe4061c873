package time;

import java.util.Scanner;

public  class Time{

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int n=0;
		int ji=0,ou=0;
		while(n!=-1){
			n=input.nextInt();
			if(n>0&&n<10000)
			{
				if(n%2==0)
				{
					ou++;
				}
				else
					ji++;
			}
			         }
		System.out.println(ji+" "+ou);

	}
}