package resolve;

import java.util.Scanner;

public class Resolve {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input = new Scanner(System.in);
		int n=input.nextInt();
		boolean isprime=true;
		int []a=new int[10];
		int count=0;
		int i;
		if(n>=2&&n<=100000){
			for(i=2;i<n/2;i++){
				if(n%i==0){
					isprime=false;
					a[count]=i;
					System.out.println(a[count]);
					count++;
					break;
				}
			}
			if(isprime==true){
				System.out.println(n+'='+n);
			}
			else
			{
				a[count]=a[count-1]/2;
				if(i=2;i<a[count];i++){
					if(n%i==0){
						isprime=false;
						a[count]=i;
						System.out.println("x"+a[count]);
						count++;
						break;
					}
				}
			}
				
		}

	}

}
